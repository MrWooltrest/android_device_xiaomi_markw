// Camera
cc_library_shared {
    name: "libcamshim",
    proprietary: true,
    compile_multilib: "32",
    defaults: ["hidl_defaults"],
    srcs: [
        "libshims.cpp",
    ],
    cflags: [
        "-Werror",
        "-Wextra",
        "-Wall",
    ],
    shared_libs: [
        "libutils",
    ],
}

cc_library_shared {
    name: "libppeiscore_shim",
    srcs: ["GraphicBuffer.cpp"],
    shared_libs: ["libui"],
    vendor: true
}


// Goodix
cc_library_shared {
    name: "fakelogprint",
    srcs: ["fakelogprint.cpp"],
    shared_libs: ["liblog"],
    compile_multilib: "64",
    vendor: true
}

cc_library_shared {
    name: "libshims_gxfpd",
    srcs: ["gxfpd_shim.cpp"],
    compile_multilib: "64",
    vendor: true
}
